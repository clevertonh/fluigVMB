      <eventDescription>function beforeStateEntry(sequenceId){&#xd;
       //VARIAVEIS DEFAULT&#xd;
       var ABERTURA = 0;&#xd;
       var APROVACAO = 5;&#xd;
       var ASSUMIR = 42;&#xd;
       &#xd;
       //GATEWAY&#xd;
       var GATEWAYAPROVADO = 14;&#xd;
       &#xd;
       //RECUPERA NUMERO DA ATIVIDADE&#xd;
       var ativAtual               = getValue(&quot;WKNumState&quot;);        &#xd;
       //RECUPERA CODIGO DA SOLICITAÇÃO&#xd;
